{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
      
    ],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-unused-vars": "error",
        "no-unused-expressions":"error",
        "prefer-const":"error",
        "no-console":"warn",
        "no-undefined":"error",
        "@typescript-eslint/no-explicit-any":"warn"
    },
    "globals": {
        "process":"readonly"
    }
  }